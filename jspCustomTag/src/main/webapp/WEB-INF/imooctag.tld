<?xml version="1.0" encoding="UTF-8" ?>
<taglib>
    <tlib-version>1.0</tlib-version><!-- 自定义标签的版本数 -->
    <short-name>SimpleTagLibrary</short-name> <!-- 标签的简写 -->
    <uri>/imooc-tag</uri><!-- 指定这个标签的uri信息 -->
    <tag>
        <name>loginDate</name><!-- 标签名字 -->
        <tag-class>com.imooc.tag.LoginDateTag</tag-class><!-- 指定标签的类 -->
        <body-content>empty</body-content><!-- 标签主体的内容 empty表示这个标签可以直接结尾,不需要填写内容  jsp表示标签体由其他jsp元素组成 -->
    </tag>
</taglib>